<main id="catalog">
    <section class="game-market">
        <h1>Game Trade Code</h1>
        <div class="offer-list">
            {{#if games}}
            {{#each games}}
                {{>gameCard}}
            {{/each}}
            {{else}}
                {{>noGames}}
            {{/if}}
        </div>
    </section>
</main>